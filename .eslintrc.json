{
    "env": {
        "browser": false,
        "commonjs": false,
        "es6": false,
        "node": false,
        "jQuery": false
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": "off",
        "curly": "off",
        "quotes": [
            "off",
            "double"
        ],        
        "no-const-assign": "off",
        "no-this-before-super": "off",
        "no-undef": "off",
        "no-unreachable": "off",
        "no-unused-vars": "off",
        "constructor-super": "off",
        "valid-typeof": "off"
    }
}